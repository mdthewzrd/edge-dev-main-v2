{
  "name": "edge-dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5665",
    "dev:renata": "next dev -p 5445",
    "dev:instagram": "next dev -p 8181",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:chromium": "playwright test --project=chromium-desktop",
    "test:firefox": "playwright test --project=firefox-desktop",
    "test:webkit": "playwright test --project=webkit-desktop",
    "test:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:tablet": "playwright test --project=tablet-chrome",
    "test:visual": "playwright test tests/visual",
    "test:e2e": "playwright test tests/e2e",
    "test:basic": "playwright test tests/e2e/page-load-basic.spec.js",
    "test:charts": "playwright test tests/e2e/charts",
    "test:trading": "playwright test tests/e2e/trading",
    "test:mobile-responsive": "playwright test tests/e2e/mobile",
    "test:performance": "playwright test --project=performance-chrome",
    "test:parallel": "playwright test --workers=4",
    "test:serial": "playwright test --workers=1",
    "test:retry": "playwright test --retries=3",
    "test:timeout": "playwright test --timeout=60000",
    "test:grep": "playwright test --grep",
    "test:reporter-html": "playwright test --reporter=html",
    "test:reporter-json": "playwright test --reporter=json",
    "test:reporter-junit": "playwright test --reporter=junit",
    "test:update-snapshots": "playwright test --update-snapshots",
    "test:install": "playwright install",
    "test:install-deps": "playwright install-deps",
    "test:show-report": "playwright show-report",
    "test:show-trace": "playwright show-trace",
    "test:codegen": "playwright codegen http://localhost:6565",
    "test:setup": "npm run test:install && npm run test:install-deps",
    "test:ci": "playwright test --reporter=json --reporter=junit --reporter=html",
    "test:smoke": "playwright test tests/e2e/page-load-basic.spec.js --project=chromium-desktop",
    "test:full": "playwright test --project=chromium-desktop --project=firefox-desktop --project=webkit-desktop --project=mobile-chrome",
    "test:clean": "rm -rf test-results && rm -rf playwright-report",
    "test:validate": "playwright test --dry-run",
    "test:list": "playwright test --list",
    "dev:test": "concurrently \"npm run dev\" \"npm run test:headed\"",
    "ci:test": "npm run build && npm run test:ci",
    "dev:full": "./dev-start.sh",
    "dev:backend": "cd backend && source venv/bin/activate && uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "health": "node scripts/health-check.js",
    "validate": "node scripts/validation-gate.js",
    "startup": "./dev-start.sh",
    "check": "npm run validate"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.34.0",
    "@copilotkit/react-core": "^1.50.1",
    "@copilotkit/react-textarea": "^1.50.1",
    "@copilotkit/react-ui": "^1.50.1",
    "@copilotkit/runtime": "^1.50.1",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.90.5",
    "@uploadthing/react": "^7.3.3",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.547.0",
    "monaco-editor": "^0.55.1",
    "next": "16.0.0",
    "openai": "^4.104.0",
    "playwright": "^1.57.0",
    "plotly.js": "^3.1.2",
    "puppeteer": "^24.32.1",
    "python-shell": "^5.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "^10.1.0",
    "react-plotly.js": "^2.6.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "undici": "^7.16.0",
    "uploadthing": "^7.7.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/plotly.js": "^3.0.7",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-plotly.js": "^2.6.3",
    "concurrently": "^9.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "jsdom": "^27.0.1",
    "node-fetch": "^3.3.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
